1659566873523:# Chunk 2
1659566873523:### Specify parameters
1659566873523:## Proportion in each compartment: Susceptible 0.999999, Infected 0.000001, Recovered 0 (these are directly from the example linked above)
1659566873523:init <- c(S = 1-1e-6, I = 1e-6, R = 0.0)
1659566873524:## beta: infection parameter; gamma: recovery parameter
1659566873524:parameters <- c(beta = 2.316, gamma = 0.261)
1659566873524:## Time frame
1659566873524:times      <- seq(0, 100, by = 0.5)
1659566873525:# Chunk 3
1659566873525:## Build the function with all
1659566873525:sir <- function(time, init, parameters) {
1659566873525:with(as.list(c(init, parameters)), {
1659566873525:dS <- -beta * S * I
1659566873525:dI <-  beta * S * I - gamma * I
1659566873526:dR <-  gamma * I
1659566873526:return(list(c(dS, dI, dR)))
1659566873526:})
1659566873526:}
1659566873526:# Chunk 4
1659566873526:## Solve using `deSolve::ode()`
